{
  "name": "blink/blink",
  "description": "A high performance web framework and application server in PHP",
  "keywords": [
    "framework",
    "swoole",
    "restful"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jin Hu",
      "email": "bixuehujin@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "nikic/fast-route": "1.2.*",
    "monolog/monolog": "^2.0",
    "psr/http-message": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "symfony/console": "^5.4",
    "psy/psysh": "^0.11",
    "symfony/dotenv": "^5.4",
    "psr/container": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "phpstan/phpstan": "^0.12.18",
    "friendsofphp/php-cs-fixer": "^2.18"
  },
  "suggest": {
    "ext-swoole": "Run blink application under Swoole with visible performance improvements"
  },
  "autoload": {
    "files": [
      "src/expression/helpers.php",
      "src/support/helpers.php"
    ],
    "psr-4": {
      "blink\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "blink\\tests\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "vendor-dir": "vendor",
    "preferred-install": "dist"
  }
}
